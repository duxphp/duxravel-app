{
  "name": "duxphp/duxravel-app",
  "description": "Duxravel Application installer",
  "type": "composer-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "DuxPHP",
      "email": "admin@duxphp.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require": {
    "php": ">=7.4.0",
    "ext-bcmath": "*",
    "ext-fileinfo": "*",
    "ext-json": "*",
    "ext-sockets": "*",
    "composer-plugin-api": "^1.1||^2.0",

  },
  "require-dev": {

  },
  "autoload": {
    "psr-4": {
      "Duxravel\\Composer\\": "src/composer",
      "Duxravel\\Core\\": "src/core"
    },
    "files": [
      "src/core/Util/Function.php"
    ]
  },
  "extra": {
    "branch-alias": {
    },
    "laravel": {
      "providers": [
        "Duxravel\\Core\\Providers\\CoreServiceProvider"
      ]
    },
    "class": "Duxravel\\Composer\\Plugin"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true,
    "preferred-install": "dist"
  }
}